{
  "name": "tuft",
  "version": "1.0.0-alpha.15",
  "description": "An HTTP/2 web framework for Node.js.",
  "author": "Stuart Kennedy",
  "license": "MIT",
  "homepage": "https://tuft.dev",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuftjs/tuft.git"
  },
  "bugs": {
    "url": "https://github.com/tuftjs/tuft/issues"
  },
  "keywords": [
    "tuft",
    "tuftjs",
    "node",
    "http2",
    "HTTP/2",
    "web",
    "framework"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run test && npm run build",
    "build": "rm -rf ./lib && tsc -b",
    "start-http2:body-parser": "ts-node ./examples/http2/body-parser.ts",
    "start-http2:cookie-parser": "ts-node ./examples/http2/cookie-parser.ts",
    "start-http2:file": "ts-node ./examples/http2/file.ts",
    "start-http2:hello-world": "ts-node ./examples/http2/hello-world.ts",
    "start-http2:search-params": "ts-node ./examples/http2/search-params.ts",
    "start-http2:static-files": "ts-node ./examples/http2/static-files.ts",
    "start-http2:stream-responder": "ts-node ./examples/http2/stream-responder.ts",
    "start-http2:wildcard-routes": "ts-node ./examples/http2/wildcard-routes.ts",
    "start-http1:body-parser": "ts-node ./examples/http1/body-parser.ts",
    "start-http1:cookie-parser": "ts-node ./examples/http1/cookie-parser.ts",
    "start-http1:file": "ts-node ./examples/http1/file.ts",
    "start-http1:hello-world": "ts-node ./examples/http1/hello-world.ts",
    "start-http1:search-params": "ts-node ./examples/http1/search-params.ts",
    "start-http1:static-files": "ts-node ./examples/http1/static-files.ts",
    "start-http1:stream-responder": "ts-node ./examples/http1/stream-responder.ts",
    "start-http1:wildcard-routes": "ts-node ./examples/http1/wildcard-routes.ts"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
